{
  "name": "naetverk",
  "version": "1.4.4",
  "description": "JavaScript framework",
  "main": "build/naetverk.common.js",
  "module": "build/naetverk.esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rete --build naetverk.config.js",
    "build:dev": "rete --build naetverk.config.js --watch",
    "prepublishOnly": "npm run lint && npm run build",
    "prebuild": "npm run lint & npm test && npm run types",
    "lint": "tsc && eslint --ext .ts --ext .js src",
    "test": "mocha -r ts-node/register test/**.ts",
    "coverage": "nyc npm run test",
    "types": "tsc src/index.ts --target es5 --declaration --outDir ./types --downlevelIteration --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naetverkjs/naetverk.git"
  },
  "keywords": [
    "dataflow",
    "visual programming",
    "node editor",
    "ts"
  ],
  "author": "Konrad Koch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naetverkjs/naetverk/issues"
  },
  "homepage": "https://github.com/naetverkjs/naetverk#readme",
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@types/jsdom": "^12.2.2",
    "@types/mocha": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^6.11.0",
    "jsdom": "^13.2.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.0.5",
    "rete-cli": "0.6.0-rc.5",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  }
}
